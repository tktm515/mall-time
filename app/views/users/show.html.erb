<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= @user.family_name + @user.first_name "さんの情報"%>
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= @user.family_name %></td>
            <td class="table__col2"><%= @user.first_name %></td>
            
          </tr>
          <tr>
            <th class="table__col1">Email</th>
            <td class="table__col2"><%= @user.email %></td>
          </tr>
          <tr>
            <th class="table__col1">電話番号</th>
            <td class="table__col2"><%= @user.phone_num %></td>
          </tr>
          <tr>
            <th class="table__col1">住所/th>
            <td class="table__col2"><%= @user.prefecture.name %></td>
            <td class="table__col2"><%= @user.city %></td>
            <td class="table__col2"><%= @user.address %></td>
            <td class="table__col2"><%= @user.building_name %></td>
          </tr>
          <tr>
            <th class="table__col1">住所/th>
            <td class="table__col2"><%= @user.birthday %></td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%#<%= @user.name + "さんのプロトタイプ"%>
      </h2>
      <div class="user__card">
        <%# 部分テンプレートでそのユーザーが投稿したプロトタイプ投稿一覧を表示する %>
        <%#<%= render partial: "prototypes/prototype", collection: @prototypes %>
      </div>
    </div>
  </div>
</div>